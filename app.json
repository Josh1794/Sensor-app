{
  "expo": {
    "name": "Sensors",
    "slug": "sensor-app",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./client/assets/icon.png",
    "splash": {
      "image": "./client/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "smartphone.sensor.app"
    },
    "description": "An app allowing users to explore the sensors in a modern smartphone",
    "githubUrl": "https://github.com/Josh1794/Sensor-app"
  }
}
